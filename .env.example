# Ewheel Importer Configuration
# Copy this file to .env and fill in your values

# Ewheel.es API Key (required)
EWHEEL_API_KEY=your-ewheel-api-key-here

# Google Translate API Key (required for translation)
GOOGLE_TRANSLATE_API_KEY=your-google-translate-api-key-here

# EUR to RON Exchange Rate
EUR_RON_RATE=4.97

# Markup Percentage (e.g., 20 for 20%)
MARKUP_PERCENT=20
